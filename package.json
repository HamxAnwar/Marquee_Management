{
  "name": "marquee-management-system",
  "version": "1.0.0",
  "description": "Professional marquee booking and event management system",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd backend && python manage.py runserver",
    "frontend": "cd frontend && npm run dev",
    "install-all": "cd backend && pip install -r requirements.txt && cd ../frontend && npm install",
    "build": "cd frontend && npm run build",
    "start": "npm run dev"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "marquee",
    "booking",
    "event-management",
    "django",
    "nextjs",
    "react"
  ],
  "author": "Your Name",
  "license": "MIT"
}